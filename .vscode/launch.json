    "version": "0.2.0",
    "configurations": [
        {
            "name": "(lldb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/webserver",
            "args": ["4000"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [{ "name": "LD_LIBRARY_PATH", "value": "net"}],
            "externalConsole": false,
            "MIMode": "lldb"
        }
    ]
}
